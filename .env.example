# API Keys
GOOGLE_API_KEY=your_gemini_api_key_here
GROQ_API_KEY=your_groq_api_key_here

# Environment
NODE_ENV=production

# Free Tier Optimizations
# Set to 'true' to enable memory-saving mode for free hosting
FREE_TIER_MODE=true

# Video Processing Limits (Free Tier)
MAX_VIDEO_SIZE_MB=10
MAX_VIDEO_DURATION_SEC=15
MAX_FRAMES=10
FRAME_FPS=1

# Paid Tier Settings (when FREE_TIER_MODE=false)
# MAX_VIDEO_SIZE_MB=50
# MAX_VIDEO_DURATION_SEC=60
# MAX_FRAMES=20
# FRAME_FPS=2
